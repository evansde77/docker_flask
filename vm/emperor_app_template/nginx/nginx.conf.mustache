server {
    listen 8080;
    server_tokens off;
    server_name restful_app;

     location /restful {
         include uwsgi_params;
         uwsgi_pass 127.0.0.1:3031;
     }

     location /configured {
        include uwsgi_params;
         uwsgi_pass 127.0.0.1:3032;

     }

     location /static {
         alias /var/www/mysite/static;
     }

}